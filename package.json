{
  "name": "howlongcanyoulive",
  "sideEffects": [
    "*.less",
    "*.css"
  ],
  "version": "1.0.0",
  "description": "How Long Can You Live",
  "main": "src/server/index.js",
  "scripts": {
    "clientstart": "webpack-dev-server --hot --mode development --open --color --config webpack.dev.js",
    "clientbuild": "webpack --mode production  --optimize-minimize --config webpack.prod.js",
    "serverstart": "nodemon src/server/index.js --exec babel-node --presets env",
    "serverbuild": "npx babel src/server -d dist/server --no-babelrc --presets env,react --copy-files --plugins=babel-plugin-transform-runtime,transform-object-rest-spread",
    "pm2start": "set NODE_ENV=production && pm2 start dist/server/index.js --node-args='--harmony'",
    "pm2restart": "set NODE_ENV=production && pm2 restart dist/server/index.js --node-args='--harmony'",
    "pm2stop": "set NODE_ENV=production && pm2 stop dist/server/index.js --node-args='--harmony'",
    "prettier-watch": "npx onchange '**/*.js' '!./dist/**/*.js' '**/*.json' '!./dist/**/*.json' '**/*.css' '!./dist/**/*.css' '**/*.less' '!./dist/**/*.less' -- prettier --write {{changed}}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Molly2Molly/howlongcanyoulive.git"
  },
  "keywords": [
    "amazing",
    "document",
    "react",
    "webpack",
    "express"
  ],
  "author": "zhangyingzi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Molly2Molly/howlongcanyoulive/issues"
  },
  "homepage": "https://github.com/Molly2Molly/howlongcanyoulive#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "csv-loader": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "less": "^3.8.1",
